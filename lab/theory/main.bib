@inproceedings{ABG+21_PostQuantumMultiPartyComputation,
  title = {Post-{{Quantum Multi}}-{{Party Computation}}},
  booktitle = {Advances in {{Cryptology}} \textendash{} {{EUROCRYPT}} 2021},
  author = {Agarwal, Amit and Bartusek, James and Goyal, Vipul and Khurana, Dakshita and Malavolta, Giulio},
