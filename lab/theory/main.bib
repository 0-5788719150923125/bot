@inproceedings{GKR08_OneTimePrograms,
  title = {One-{{Time Programs}}},
  booktitle = {Advances in {{Cryptology}} – {{CRYPTO}} 2008},
  author = {Goldwasser, Shafi and Kalai, Yael Tauman and Rothblum, Guy N.},
